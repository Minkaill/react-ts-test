# Production окружение
NODE_ENV=production

# API конфигурация для продакшена
REACT_APP_API_URL=https://api.yourdomain.com/api
REACT_APP_API_TIMEOUT=15000

# Настройки приложения
REACT_APP_NAME="React TS Boilerplate"
REACT_APP_VERSION=1.0.0

# Функциональность для продакшена
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_ERROR_REPORTING=true
REACT_APP_ENABLE_DEV_TOOLS=false

# Бизнес-логика
REACT_APP_DEFAULT_CURRENCY=USD
REACT_APP_CURRENCY_SYMBOL=$
REACT_APP_DEFAULT_PAGE_SIZE=12
REACT_APP_MAX_PAGE_SIZE=100
REACT_APP_MAX_CART_ITEMS=99

# Порт для продакшена
PORT=8080

# Настройки сборки для продакшена
GENERATE_SOURCEMAP=false
ANALYZE_BUNDLE=false
PUBLIC_PATH=/