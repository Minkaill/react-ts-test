# Development окружение
NODE_ENV=development

# API конфигурация для разработки
REACT_APP_API_URL=http://localhost:8000/api
REACT_APP_API_TIMEOUT=10000

# Настройки приложения
REACT_APP_NAME="React TS Boilerplate (Dev)"
REACT_APP_VERSION=1.0.0

# Функциональность для разработки
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_ERROR_REPORTING=true
REACT_APP_ENABLE_DEV_TOOLS=true

# Бизнес-логика
REACT_APP_DEFAULT_CURRENCY=USD
REACT_APP_CURRENCY_SYMBOL=$
REACT_APP_DEFAULT_PAGE_SIZE=12
REACT_APP_MAX_PAGE_SIZE=100
REACT_APP_MAX_CART_ITEMS=99

# Порт для разработки
PORT=3000

# Настройки сборки для разработки
GENERATE_SOURCEMAP=true
ANALYZE_BUNDLE=false
PUBLIC_PATH=/